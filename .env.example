APP_NAME=Statamic
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

STATAMIC_LICENSE_KEY=
STATAMIC_STACHE_WATCHER=true
STATAMIC_STATIC_CACHING_STRATEGY=null
STATAMIC_REVISIONS_ENABLED=false
STATAMIC_GRAPHQL_ENABLED=false
STATAMIC_API_ENABLED=false
STATAMIC_GIT_ENABLED=false

# Start of smj-statamic-starter env variables
# AKISMET_API_KEY=
# SENTRY_LARAVEL_DSN=
# CP_ENABLED=
## set to false to disable {{ cache }} tags while in development. As of writing, this is built into Statamic, but not in the .env by default.
STATAMIC_CACHE_TAGS_ENABLED=false
